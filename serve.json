{
  "rewrites": [
    { "source": "**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "public": ".",
  "directoryListing": false,
  "trailingSlash": false,
  "cleanUrls": true,
  "unlisted": [
    "src",
    "dist",
    "node_modules"
  ]
}